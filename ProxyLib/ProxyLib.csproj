<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Authors>Lee Harding</Authors>
    <Product>Proxy to Amazon Web Services Library</Product>
    <Description>This library provides a high-performance proxy implementation with the sole purpose of forwarding network message streams to AWS resources.  Support for UDP, TCP and SSL source streams is included, as are AWS Lambda, Firehose, SNS, SQS, S3 and StepFunctions destinations.  If you want to proxy to non-AWS destinations, this may not be the tool for you.</Description>
    <PackageId>PAWSLib</PackageId>
    <ApplicationIcon>PAWS.ico</ApplicationIcon>
    <PackageProjectUrl>https://github.com/mlhpdx/PAWS</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/mlhpdx/PAWS.git</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.Core" Version="3.7.12.2" />
    <PackageReference Include="AWSSDK.KinesisFirehose" Version="3.7.2.96" />
    <PackageReference Include="AWSSDK.Lambda" Version="3.7.13.16" />
    <PackageReference Include="AWSSDK.S3" Version="3.7.9.18" />
    <PackageReference Include="AWSSDK.SecurityToken" Version="3.7.1.167" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.7.3.74" />
    <PackageReference Include="AWSSDK.SQS" Version="3.7.2.71" />
    <PackageReference Include="AWSSDK.StepFunctions" Version="3.7.0.174" />
    <PackageReference Include="ClientPolicyLib" Version="1.0.1-build.1568409103-0cf966144" />
    <PackageReference Include="DnsClient" Version="1.4.0" />
    <PackageReference Include="IPNetwork2" Version="2.5.320" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Admin\Control\" />
  </ItemGroup>

</Project>
